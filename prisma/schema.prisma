generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model StudentPage {
  id Int @id @default(autoincrement())

  name        String
  bgImage     String   @default("")
  content     String
  orientation String   @default("")
  createdAt   DateTime @default(now())
}
